{
  "name": "@n3okill/utils",
  "version": "1.0.0",
  "description": "Many javascript helpers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Joao Parreira",
  "license": "MIT",
  "scripts": {
    "setup": "npm install",
    "clean:dist": "rimraf ./dist",
    "build:mjs": "npx tsc -p tsconfig-mjs.json",
    "build:cjs": "npx tsc -p tsconfig-cjs.json",
    "build:package.json": "node ./build.js",
    "build": "npm run clean:dist && npm run build:mjs && npm run build:cjs && npm run build:package.json",
    "lint": "eslint src --ext .ts && eslint tests --ext .ts",
    "format": "prettier --config .prettierrc 'src/**/*.ts' 'tests/**/*.ts' --write",
    "pretest": "npm run build",
    "unit": "jest --coverage --reporter=lcov --reporter=text",
    "test": "npm run lint && npm run unit",
    "prepublishOnly": "npm test",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "docs": "npm install && npm run typedoc",
    "typedoc": "typedoc --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.0",
    "jest": "^27.4.3",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.1",
    "typedoc": "^0.22.5",
    "typedoc-plugin-markdown": "^3.11.3",
    "typescript": "^4.5.2"
  },
  "files":["dist/**/*"],
  "keywords": [
    "array",
    "chunk",
    "clone",
    "deep",
    "combine",
    "unique",
    "contains",
    "diff",
    "remove",
    "delete",
    "flat",
    "intersect",
    "isEmpty",
    "range",
    "toMap",
    "toString",
    "buffer",
    "repeat",
    "trim",
    "replace",
    "format",
    "pad",
    "xor",
    "titleCase",
    "expand",
    "split",
    "version",
    "compare",
    "validate",
    "is",
    "type",
    "email"
  ]
}
